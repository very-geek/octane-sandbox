{
  "name": "octane-sandbox",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for octane-sandbox goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "embroider": "EMBROIDER=true ember serve",
    "build": "ember build --environment=production",
    "lint:deps": "ember dependency-lint",
    "lint:js": "eslint .",
    "lint:ts": "eslint . --ext .ts",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "prettier": "prettier . --write",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.hbs": "ember-template-lint --fix",
    "*.{j,t}s": "eslint --fix",
    "*.{css,scss,html,hbs,json,md,yml}": "prettier --write"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@embroider/compat": "0.25.0",
    "@embroider/core": "0.25.0",
    "@embroider/router": "0.25.0",
    "@embroider/webpack": "0.25.0",
    "@glimmer/component": "1.0.2",
    "@glimmer/tracking": "1.0.2",
    "@sentry/browser": "5.24.2",
    "@sentry/integrations": "5.24.2",
    "@types/ember": "3.16.0",
    "@types/ember-data": "3.16.4",
    "@types/ember-qunit": "3.4.11",
    "@types/ember__test-helpers": "1.7.2",
    "@types/qunit": "2.9.5",
    "@types/rsvp": "4.0.3",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "1.6.0",
    "ember-cli": "3.21.2",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-babel": "7.22.1",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-dependency-lint": "1.1.3",
    "ember-cli-htmlbars": "5.3.1",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-typescript": "4.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-data": "3.21.1",
    "ember-export-application-global": "2.0.1",
    "ember-fetch": "8.0.2",
    "ember-load-initializers": "2.1.1",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.6.0",
    "ember-resolver": "8.0.2",
    "ember-source": "3.21.1",
    "ember-template-lint": "2.12.1",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-ember": "9.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-qunit": "5.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "qunit-dom": "1.5.0",
    "tracked-built-ins": "1.0.2",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  }
}
